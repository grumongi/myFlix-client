{
  "source": {
    "include": ["./src/"],
    "exclude": ["./src/**/*.test.js", "./src/**/*.spec.js"],
    "includePattern": "\\.(js|jsx)$",
    "excludePattern": "node_modules/"
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  }
}
